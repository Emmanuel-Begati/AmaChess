{
  "name": "amachess",
  "version": "1.0.0",
  "description": "AmaChess - Full Stack Chess Application",
  "scripts": {
    "install:all": "npm install && cd amachess-backend && npm install && cd ../amachess-frontend && npm install",
    "build": "cd amachess-frontend && npm run build",
    "start": "cd amachess-backend && npm start",
    "dev": "cd amachess-backend && npm run dev",
    "dev:frontend": "cd amachess-frontend && npm run dev",
    "dev:backend": "cd amachess-backend && npm run dev",
    "build:start": "npm run build && npm start",
    "deploy": "node deploy.js"
  },
  "dependencies": {
    "node-fetch": "^2.7.0"
  },
  "keywords": ["chess", "stockfish", "react", "express", "fullstack"],
  "author": "AmaChess",
  "license": "MIT"
}
